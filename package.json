    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discovery-solutions/json-server.git"
  },
  "bugs": {
    "url": "https://github.com/discovery-solutions/json-server/issues"
  }
