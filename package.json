{
  "name": "@discovery-solutions/json-server",
  "version": "2.0.0",
  "description": "A Node Framework for API Development Based in JSON",
  "main": "./src/index.js",
  "author": "Lucas Craveiro Paes",
  "license": "ISC",
  "homepage": "https://github.com/discovery-solutions/json-server#readme",
  "scripts": {
